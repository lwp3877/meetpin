{
  "name": "meetpin",
  "version": "1.5.0",
  "description": "지도에서 방을 만들어 근처 사람들과 만나고, 새로운 인연을 만들어보세요",
  "keywords": [
    "nextjs",
    "supabase",
    "kakao-maps",
    "stripe",
    "social",
    "meeting",
    "location-based"
  ],
  "author": "MeetPin Team",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "restart:clean": "rimraf .next && next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:all": "playwright test",
    "test:e2e:prod": "playwright test --project=chromium --project=firefox",
    "qa:scan": "node scripts/dom-scan.mjs",
    "db:migrate": "echo 'Run scripts/migrate.sql in your Supabase SQL editor'",
    "db:rls": "echo 'Run scripts/rls.sql in your Supabase SQL editor'",
    "db:seed": "echo 'Run scripts/seed.sql in your Supabase SQL editor'",
    "repo:doctor": "pnpm typecheck && pnpm lint && pnpm arch:check && pnpm build",
    "arch:check": "node scripts/check-architecture.js",
    "approve-builds": "echo 'Build approved for pnpm run commands'",
    "playwright:install": "playwright install",
    "preview": "next build && next start",
    "dev:check": "next build && next start -p 3000 & wait-on http://localhost:3000 && node scripts/check-routes.mjs",
    "qa:local": "playwright test --config=playwright.config.ts",
    "qa:production": "TEST_URL=https://meetpin-weld.vercel.app playwright test e2e/production-quick-test.spec.ts",
    "qa:detailed": "TEST_URL=https://meetpin-weld.vercel.app playwright test e2e/detailed-production-test.spec.ts",
    "qa:performance": "playwright test e2e/performance-security.spec.ts --project=chromium",
    "qa:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "qa:report": "playwright show-report",
    "qa:setup": "node scripts/qa-setup.js && playwright install",
    "qa:validate": "pnpm typecheck && pnpm lint && pnpm build && pnpm test",
    "qa:full": "pnpm qa:validate && pnpm qa:local && pnpm qa:production",
    "analyze:bundle": "cross-env ANALYZE=1 next build",
    "perf:baseline": "node scripts/perf-baseline.mjs",
    "perf:compare": "node scripts/perf-compare.mjs",
    "perf:guard": "pnpm perf:baseline && pnpm perf:compare",
    "perf:slow4g": "playwright test tests/perf/slow4g.spec.ts",
    "sentry:upload": "node scripts/sentry-upload.js",
    "smoke": "playwright test e2e/smoke.spec.ts",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "bundle:guard": "node scripts/bundle-guard.js",
    "audit:security": "pnpm audit --audit-level moderate",
    "a11y": "playwright test tests/e2e/07-accessibility.spec.ts",
    "a11y:report": "playwright test tests/e2e/07-accessibility.spec.ts --reporter=html"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.15.0",
    "@sentry/webpack-plugin": "^4.3.0",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "intersection-observer": "^0.12.2",
    "ioredis": "^5.8.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "playwright": "^1.56.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.1.2",
    "@next/eslint-plugin-next": "^15.5.4",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "knip": "^5.64.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^5.0.10",
    "server-only": "^0.0.1",
    "sharp": "^0.34.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^7.2.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
